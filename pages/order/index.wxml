<tabs tabs="{{tabs}}" binditemChange="handleItemChange">
        <view class="order_list">
            <view class="order_item" wx:for="{{orderList}}" wx:key="order_id">
                <view class="item_num item_list">
                    <view class="num_name">订单编号</view>
                    <view class="num_value">{{item.order_number}}</view>
                </view>
                <view class="item_price item_list">
                    <view class="price_name">订单价格</view>
                    <view class="price_value">￥{{item.order_price}}</view>
                </view>
                <view class="item_time item_list">
                    <view class="time_name">订单日期</view>
                    <view class="time_value">{{item.newdate}}</view>
                </view>
            </view>
        </view>
</tabs>